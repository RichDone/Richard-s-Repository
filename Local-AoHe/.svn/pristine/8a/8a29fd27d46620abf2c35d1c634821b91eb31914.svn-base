#loading {
    display: block;
    opacity: 0.6;
    filter: alpha(opacity=60);
    z-index: 10;
    text-align: center;
    vertical-align: middle;
    background-color: gray;
    position: absolute;
    top: 0;
    left: 0;
}

#txtLoading {
    z-index: 11;
    color: white;
    position: absolute;
    width: 250px;
    height: 50px;
    display: inline-block;
    background-color: black;
    text-align: center;
    line-height: 50px;
    font-size:12px;
    font-weight:bold;
}
.schedual_change {
    z-index: 11;
    color: black;
